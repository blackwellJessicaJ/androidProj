<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fitnessapplication">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".EditNote"
            android:label="@string/title_edit_note"/>
        <activity android:name=".AddNote" />
        <activity
            android:name=".NotePreview"
            android:label="@string/title_note_preview" />
        <activity
            android:name=".CalendarPage"
            android:label="@string/title_activity_calendar" />
        <activity
            android:name=".Workouts"
            android:label="@string/title_activity_workouts" />
        <activity android:name=".FirstOpen" />
        <activity
            android:name=".ProfileEdit"
            android:label="@string/title_activity_profile_edit" />
        <activity
            android:name=".Profile"
            android:label="@string/title_activity_profile" />
        <activity
            android:name=".MainActivity"
            android:label="@string/title_home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".WorkoutRecycler"
            android:label="Workout Recycler" />
    </application>

</manifest>